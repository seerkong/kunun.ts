[main {
    [table val field {
        [field m]
        [field n = 1]
      }
      prop {
        [field_prop !public m ~Int]
        [field_prop !public n ~Int]
        [calc_prop o ~Int !public set !(m) (value) {
            [set ~m value]
          }
          !public get !(m) {
            m
          }
        ]
        [calc_prop p ~Int !public set !(m n) (value) {
            [set ~m value]
            [set ~n value]
          }
          !public get !(m n) {
            [+ (m n)]
          }
        ]
      }
      method {
        [method !public q (a1 ~Int a2 ~Int) ~{Int} {
            [* (m a1 a2)]
          }
        ]
      }
    ]
    val
    [val @.n = 2]
    [val .n]
  }
]