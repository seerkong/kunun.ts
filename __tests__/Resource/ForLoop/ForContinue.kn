[do {
    [var ~a ${1 2 3}]
    [var ~b 0]
    [for
      %
      i = 0
      %
      [i
        [a .length]
        <;
      ]
      [++ i] {
        [var ~x
          [a .:i]
        ]
        [if
          [== (x 2)] {
            [continue;]
          }
        ]
        [Writeln (x)]
        [set ~b x]
      }
    ]
    b
  }
]